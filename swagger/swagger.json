    "definitions": {
        "define.ProfileResult": {
            "type": "object",
            "properties": {
                "avatar": {
                    "description": "头像",
                    "type": "string"
                },
                "nickName": {
                    "description": "昵称",
                    "type": "string"
                }
            }
        },
        "define.UserQuery": {
            "type": "object",
            "required": [
                "page",
                "size"
            ],
            "properties": {
                "nickName": {
                    "description": "昵称",
                    "type": "string"
                },
                "page": {
                    "description": "页码",
                    "type": "integer"
                },
                "size": {
                    "description": "每页条数",
                    "type": "integer"
                }
            }
        },
        "response.JsonResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "data": {
                    "type": "object"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "ApiKeyAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
